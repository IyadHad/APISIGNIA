{
  "pythonVersion": "3.9",
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "requirements.txt",
      "use": "@now/python",
      "config": { "pip": "pip3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ]
}
